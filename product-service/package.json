{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless Shop BE",
  "main": "serverless.ts",
  "scripts": {
    "generate-docs": "sls generate-swagger",
    "test": "jest --silent",
    "deploy": "npm run test && sls deploy",
    "fill-tables": "ts-node src/libs/fill-db-tables.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.428.0",
    "@aws-sdk/lib-dynamodb": "^3.430.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.5.5",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "jest": "^29.7.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.12.0",
    "serverless-webpack": "^5.13.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.1.3",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@libs/(.*)$": "<rootDir>/src/libs/$1",
      "^@functions/(.*)$": "<rootDir>/src/functions/$1",
      "^@mocks/(.*)$": "<rootDir>/src/mocks/$1"
    }
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
